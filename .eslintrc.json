{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "padded-blocks": "off",
    "no-multi-assign": "off",
    "consistent-return": "off",
    "prefer-arrow-callback": "off",
    "prefer-destructuring": ["off", {
      "array": true,
      "object": true
    }, {
      "enforceForRenamedProperties": false
    }]
  }
}
